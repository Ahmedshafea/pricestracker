// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id           String    @id @default(uuid())
  url          String    @unique
  title        String?
  price        Float?
  availability Boolean?
  image_url    String?
  createdAt    DateTime  @default(now())
}